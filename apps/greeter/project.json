{
  "root": "apps/greeter",
  "sourceRoot": "apps/greeter/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build -o ./bin/greeter.exe ./src",
        "cwd": "apps/greeter"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go run ./src",
        "cwd": "apps/greeter"
      }
    }
  }
}
