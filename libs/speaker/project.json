{
  "root": "libs/speaker",
  "sourceRoot": "libs/speaker/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build -o ./bin/greeter.exe ./src",
        "cwd": "libs/speaker"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go test ./src",
        "cwd": "libs/speaker"
      }
    }
  }
}
